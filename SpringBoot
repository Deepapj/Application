  	<script type="text/javascript">
  	
  	 $(document).ready(function () {
   		$('input:radio[name="language"]').change(function () {
              var selectedOption = $(this).val();
              if (selectedOption != '') {
             	 $('#haoagentForm').attr('action', '/hao/haoagentLanguageChange?lang='+selectedOption);
             	 $('#haoagentForm').submit();
             	 
              }
          });
      });
  	 
  	
	function retrieveSymptoms() {
  	  	var url = '/hao/symptomsTree';
  	  	var data = {}
  	  	data["symptoms"] = $("#symptoms").val();
  		var token = $('#_csrf').attr('content');
		var header = $('#_csrf_header').attr('content');
		
		var e = document.getElementById("symptoms");
		var optionSelIndex = e.options[e.selectedIndex].value;
		var optionSelectedText = e.options[e.selectedIndex].text;
		if(optionSelIndex == 0)
			{
			alert("!!Please select the symptom first!!");
			}
		$.ajax({
		    type: "POST",
		    url: url,
		    data: data,
		    beforeSend: function (xhr) {
		        xhr.setRequestHeader(header, token);
		    },
		    success: function (data, textStatus, jqXHR) {
		    	loadSymptomsTree(data);
		    },
		    error: function (request, status, error) {
		        alert(status);
		    }
		});
	}
  	
	function retrieveAllSymptoms() {
  	  	var url = '/hao/symptomsTreeAll';
		var token = $('#_csrf').attr('content');
		var header = $('#_csrf_header').attr('content');

		$.ajax({
		    type: "POST",
		    url: url,
		    beforeSend: function (xhr) {
		        xhr.setRequestHeader(header, token);
		    },
		    success: function (data, textStatus, jqXHR) {
		    	loadAllSymptomsTree(data);
		    	$("#fileViewModal").modal();
		    },
		    error: function (request, status, error) {
		        alert(status);
		    }
		});
  	}
	
	function retrieveAllDiseaseInfo() {
  	  	var url = '/hao/allDiseaseInfo';
		var token = $('#_csrf').attr('content');
		var header = $('#_csrf_header').attr('content');

		$.ajax({
		    type: "POST",
		    url: url,
		    beforeSend: function (xhr) {
		        xhr.setRequestHeader(header, token);
		    },
		    success: function (data, textStatus, jqXHR) {
		    	loadAllSymptomsTree(data);
		    	$("#fileViewModal").modal();
		    	$("#diseaseSummaryProvided").val("Through All Summary List - ");
		    },
		    error: function (request, status, error) {
		        alert(status);
		    }
		});
  	}
	
	function retrieveDiseaseInfo(node) {
	  	  	var url = '/hao/diseaseInfo';
			var token = $('#_csrf').attr('content');
			var header = $('#_csrf_header').attr('content');
			var data = JSON.stringify(node);

			$.ajax({
			    type: "POST",
			    data: data,
			    contentType: "application/json; charset=utf-8",
                dataType: "json",
			    url: url,
			    beforeSend: function (xhr) {
			        xhr.setRequestHeader(header, token);
			    },
			    success: function (data, textStatus, jqXHR) {
			    	loadAllSymptomsTree(data);
			    	$("#fileViewModal").modal();
			    	$("#diseaseSummaryProvided").val("Through Summary List - ");
			    },
			    error: function (request, status, error) {
			        alert(status);
			    }
			});
	  	}
  	
	function feedbackSave(){
  		var url = '/hao/feedback';
 		 var form = $('#feedbackForm');
 		var token = $('#_csrf').attr('content');
		var header = $('#_csrf_header').attr('content');
 		   $.ajax({
 			    type: "POST",
 			    url: url,
 			    data: $('#feedbackForm').serialize(),
 			    beforeSend: function (xhr) {
 			        xhr.setRequestHeader(header, token);
 			    },
 			    success: function (data, textStatus, jqXHR) {
 			    	alert("Feedback saved Successfully");
 			    	$('#feedbackModal').modal('toggle');
 			    },
 			    error: function (request, status, error) {
 			        alert(status);
 			    }
 			});
  	 }
  	
  	 function feedbackModal(){
  		 $("#feedbackCallerName").val($("#name").val());
  	 }
  	 
  	
  	
  	function complaintSave(){
  		var url = '/hao/complaint';
 		var form = $('#complaintForm');
 		var token = $('#_csrf').attr('content');
		var header = $('#_csrf_header').attr('content');
		
 		   $.ajax({
 			    type: "POST",
 			    url: url,
 			    data: $('#complaintForm').serialize(),
 			    beforeSend: function (xhr) {
 			        xhr.setRequestHeader(header, token);
 			    },
 			    success: function (data, textStatus, jqXHR) {
 			    	alert("Complaint saved Successfully");
 			    	$('#complaintModal').modal('toggle');
 			    },
 			    error: function (request, status, error) {
 			        alert(status);
 			    }
 			});
  	 } 
  	
  	 function complaintModal(){
  		 $("#complaintCallerName").val($("#name").val());
  	 }
  	 
  	
