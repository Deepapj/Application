  	function retrieveSubType() {
  	    var url = '/hao/retrieveSubType';
  	  var data = {}
  	  data["selectedCallType"] = $("#callType").val();
		var token = $('#_csrf').attr('content');
		var header = $('#_csrf_header').attr('content');
		$.ajax({
		    type: "POST",
		    url: url,
		    data: data,
		    beforeSend: function (xhr) {
		        xhr.setRequestHeader(header, token);
		    },
		    success: function (data, textStatus, jqXHR) {
		    	$("#subTypeDiv").html(data);
		    },
		    error: function (request, status, error) {
		        alert(status);
		    }
		});
  	}
  	
  	function retrieveFile() {
  	    var url = '/hao/symptomsFile';
  	  var data = {}
  	  data["symptoms"] = "external";
		var token = $('#_csrf').attr('content');
		var header = $('#_csrf_header').attr('content');

		$.ajax({
		    type: "GET",
		    url: url,
		    data: data,
		    beforeSend: function (xhr) {
		        xhr.setRequestHeader(header, token);
		    },
		    success: function (data, textStatus, jqXHR) {
		    	retrieveAllSymptoms();
		    	var fileDisplay = "/file/Display Setting.pdf"
		    	$("#fileContent").html('<embed src="'+fileDisplay+'" type="application/pdf" width="100%" height="600px"></embed>');
		    	
		    	$("#fileViewModal").modal();
		    },
		    error: function (request, status, error) {
		        alert(status);
		    }
		});
  	}
  	
  	
  	
  	function retrieveFileAll() {
  	    var url = '/hao/symptomsFile';
  	  var data = {}
  	  data["symptoms"] = "external";
		var token = $('#_csrf').attr('content');
		var header = $('#_csrf_header').attr('content');

		$.ajax({
		    type: "GET",
		    url: url,
		    data: data,
		    beforeSend: function (xhr) {
		        xhr.setRequestHeader(header, token);
		    },
		    success: function (data, textStatus, jqXHR) {
		    	var fileDisplay = "/file/Display Setting.pdf"
		    	$("#fileContent").html('<embed src="'+fileDisplay+'" type="application/pdf" width="100%" height="600px"></embed>');
		    	
		    	$("#fileViewModal").modal();
		    },
		    error: function (request, status, error) {
		        alert(status);
		    }
		});
  	}
  	
  	

  	
  	function loadSymptomsTree(dataJson){
  		dataJson = JSON.stringify(dataJson);
  		dataJson = "["+dataJson+"]";
  		 var $checkableTree = $('#treeview-checkable').treeview({
  	          data: dataJson,
  	          showIcon: true,
  	          showCheckbox: true,
  	          onNodeChecked: function(event, node) {
  	        	$('#treeview-checkable').treeview('disableAll', { silent: true });
  	        	$('#treeview-checkable').treeview('enableNode', [ node.nodeId, { silent: true } ]);
  	        	$('#treeview-checkable').treeview('selectNode', [ node.nodeId, { silent: false } ]);
  	        	retrieveDiseaseInfo(node);
  	          },
  	          onNodeUnchecked: function (event, node) {
  	        	$('#treeview-checkable').treeview('enableAll', { silent: true });

  	          },
  	          onNodeSelected: function(event, node) {
  	        	  $('#checkable-output').show();
 	        	 
	        	  $('#checkable-output-symptoms').empty();
	              $('#checkable-output-symptoms').prepend('<p>' + node.symptomsTypeDescreption + '</p>');
	              
	              $('#checkable-output-action').empty();
	              $('#checkable-output-action').prepend('<p>' + node.symptomsTypeAction + '</p>');
	              
	              $('#checkable-output-dosndonts').empty();
	              $('#checkable-output-dosndonts').prepend('<p>' + node.symptomsTypeDosAndDonts + '</p>');
	              
	              $('#checkable-output-information').empty();
	              $('#checkable-output-information').prepend('<p>' + node.symptomsTypeInformation + '</p>');
  	            },
  	            onNodeUnselected: function (event, node) {
  	            	$('#checkable-output').hide();
  	            }
  	        });

  	}
  	
  	function searchTree(){
  		$('#btn-search').click();
  	}
  	
  	function loadAllSymptomsTree(dataJson){
  		dataJson = JSON.stringify(dataJson);
  		dataJson = "["+dataJson+"]";
  		 var $checkableTree = $('#treeview-checkable1').treeview({
  	          data: dataJson,
  	          showIcon: true,
  	          showCheckbox: true,
  	          multiSelect: false,
  	          onNodeChecked: function(event, node) {
  	        	//$('#checkable-output1').hide();
  	        	$('#treeview-checkable1').treeview('disableAll', { silent: true });
  	        	$('#treeview-checkable1').treeview('enableNode', [ node.nodeId, { silent: true } ]);
  	        	$('#treeview-checkable1').treeview('selectNode', [ node.nodeId, { silent: false } ]);
  	        	$("#diseaseSummaryProvided").val($("#diseaseSummaryProvided").val() + node.text + ";");
  	        	//retrieveFileAll();
  	        	//var fileDisplay = "/file/Display Setting.pdf"
  	        	var fileDisplay = node.text;
		    	$("#fileContent").html('<embed src="/file/pdf/'+fileDisplay+'.pdf" type="application/pdf" width="100%" height="600px"></embed>');
		    	
		    	$("#fileViewModal").modal();
  	        	if($('#input-search').val()){
		    		$('#btn-search').click();
		    	}
  	          },
  	          onNodeUnchecked: function (event, node) {
  	        	$('#treeview-checkable1').treeview('enableAll', { silent: true });
  	        	$("#fileContent").empty();
  	        	//$('#checkable-output1').show();
  	        	if($('#input-search').val()){
		    		$('#btn-search').click();
		    	}
  	          },
  	          onNodeSelected: function(event, node) {
  	        	 /*  $('#checkable-output1').show();
	        	 
	        	  $('#checkable-output1-symptoms').empty();
	              $('#checkable-output1-symptoms').prepend('<p>' + node.symptomsTypeDescreption + '</p>');
	              
	              $('#checkable-output1-action').empty();
	              $('#checkable-output1-action').prepend('<p>' + node.symptomsTypeAction + '</p>');
	              
	              $('#checkable-output1-dosndonts').empty();
	              $('#checkable-output1-dosndonts').prepend('<p>' + node.symptomsTypeDosAndDonts + '</p>');
	              
	              $('#checkable-output1-information').empty();
	              $('#checkable-output1-information').prepend('<p>' + node.symptomsTypeInformation + '</p>'); */
	              
	  	            if($('#input-search').val()){
	  	            	 setTimeout(searchTree, 10);
			    	}
  	            },
  	            onNodeUnselected: function (event, node) {
  	            	/* $('#checkable-output1').hide(); */
  	            	if($('#input-search').val()){
  	            		 setTimeout(searchTree, 10);
  			    	}
  	            }
  	        });
  		$.bind($checkableTree, $.proxy($checkableTree.callback, $checkableTree));
  		var search = function(e) {
            var pattern = $('#input-search').val();
            var options = {
              ignoreCase: true
            };
            var results = $checkableTree.treeview('search', [ pattern, options ]);
            var output = '<p>' + results.length + ' matches found</p>';
            $.each(results, function (index, result) {
              output += '<p>- ' + result.text + '</p>';
            });
            $('#search-output').html(output);
          }
          $('#btn-search').on('click', search);
          $('#input-search').on('keyup', search);
          $('#btn-clear-search').on('click', function (e) {
            $checkableTree.treeview('clearSearch');
            $('#input-search').val('');
            $('#search-output').html('');
          });
          
          $("#input-search").on("keyup", function() {
        	    var value = $(this).val().toLowerCase();
        	    $("#treeview-checkable1 li").filter(function() {
        	      $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
        	    });
        	  });
          
          $("#btn-search").on("click", function() {
      	    var value = $("#input-search").val().toLowerCase();
      	    $("#treeview-checkable1 li").filter(function() {
      	      $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
      	    });
      	  });
          
          $('.has-clear input[type="text"]').on('input propertychange', function() {
        	  var $this = $(this);
        	  var visible = Boolean($this.val());
        	  $this.siblings('.form-control-clear').toggleClass('hidden', !visible);
        	}).trigger('propertychange');

        	$('.form-control-clear').click(function() {
        	  $(this).siblings('input[type="text"]').val('')
        	    .trigger('propertychange').focus();
        	});
        	
        	$("#fileContent").html('');
  	}
  	
  	
  	$('input[name="callHandling"]').change(function(){
  		if($(this).is(':checked') && $(this).val() =='yes'){
  			$('#yesModal').modal('show');
  		}
  		
  	}); 
  	
  	
  	
	function retrieveTalluks() {
  	    var url = '/hao/retrieveTalluks';
  	  var data = {}
  	  data["selectedDistrict"] = $("#district").val();
		var token = $('#_csrf').attr('content');
		var header = $('#_csrf_header').attr('content');

		$.ajax({
		    type: "POST",
		    url: url,
		    data: data,
		    beforeSend: function (xhr) {
		        xhr.setRequestHeader(header, token);
		    },
		    success: function (data, textStatus, jqXHR) {
		    	$("#tallukDiv").html(data);
		    },
		    error: function (request, status, error) {
		        alert(status);
		    }
		});
  	}
  	
  	function retrieveVillages() {
  	    var url = '/hao/retrieveVillages';
  	  var data = {}
  	  data["selectedTalluk"] = $("#talluk").val();
		var token = $('#_csrf').attr('content');
		var header = $('#_csrf_header').attr('content');

		$.ajax({
		    type: "POST",
		    url: url,
		    data: data,
		    beforeSend: function (xhr) {
		        xhr.setRequestHeader(header, token);
		    },
		    success: function (data, textStatus, jqXHR) {
		    	$("#villageDiv").html(data);
		    },
		    error: function (request, status, error) {
		        alert(status);
		    }
		});
  	}
  	
